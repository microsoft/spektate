{
  "name": "spektate",
  "version": "0.1.0",
  "main": "build/dist/index.js",
  "types": "build/dist/index.d.ts",
  "proxy": "http://localhost:8001",
  "license": "MIT",
  "dependencies": {
    "azure-devops-ui": "^2.166.1",
    "office-ui-fabric-react": "^7.10.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-scripts-ts": "^4.0.8",
    "spektate": "^1.0.11"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "lint": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/jest-when": "^2.7.0",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "husky": "^3.0.4",
    "jest-junit": "^10.0.0",
    "jest-when": "^2.7.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pkg": {
    "assets": [
      "./node_modules/@types",
      "./node_modules/typescript/lib/*.d.ts",
      "src/**/*.ts",
      "./tsconfig.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}