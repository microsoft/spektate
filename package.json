{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "azure-devops-ui": "^1.155.0",
    "cli-table": "^0.3.1",
    "commander": "^2.20.0",
    "fs": "0.0.1-security",
    "office-ui-fabric-react": "^7.10.0",
    "open": "^6.4.0",
    "os": "^0.1.1",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-scripts-ts": "^4.0.8"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "mocha -r ts-node/register src/tests/Basic.test.ts",
    "private-github-test": "mocha -r ts-node/register src/tests/PrivateGitHub.test.ts",
    "eject": "react-scripts-ts eject",
    "start-cli": "nodemon --watch 'src/cli/**/*.ts' --exec 'ts-node' src/cli/spex.ts",
    "create-cli": "npm run build-cli",
    "build-cli": "sh release-cli.sh",
    "lint": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cli-table": "^0.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "azure-storage": "^2.10.3",
    "chai": "^4.2.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pkg": {
    "assets": [
      "./node_modules/@types",
      "./node_modules/typescript/lib/*.d.ts",
      "src/**/*.ts",
      "./tsconfig.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,json,md,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
